{
  "type": "sqlite",
  "database": "db.slite3",
  "autoSchemaSync": true,
  "synchronize": true,
  "logging": true,
  "entities": [
    "scr/entity/**/*.ts"
  ],
  "subscribers": [
    "src/subscriber/**/*.ts"
  ],
  "cli": {
    "entitiesDir": "src/entity",
    "migrationsDir": "src/migration",
    "subscribersDir": "src/subscriber"
  }
}