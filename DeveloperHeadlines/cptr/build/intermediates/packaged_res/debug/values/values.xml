<?xml version="1.0" encoding="utf-8"?>
<resources>
    <string name="app_name">CommonPullToRefreshView</string>
    <string name="cube_ptr_hours_ago"> 小时之前</string>
    <string name="cube_ptr_last_update">上次更新: </string>
    <string name="cube_ptr_minutes_ago"> 分钟之前</string>
    <string name="cube_ptr_pull_down">下拉</string>
    <string name="cube_ptr_pull_down_to_refresh">下拉刷新</string>
    <string name="cube_ptr_refresh_complete">更新完成.</string>
    <string name="cube_ptr_refreshing">加载中...</string>
    <string name="cube_ptr_release_to_refresh">释放刷新</string>
    <string name="cube_ptr_seconds_ago"> 秒之前</string>
    <declare-styleable name="PtrClassicHeader">
        <attr format="integer" name="ptr_rotate_ani_time"/>
    </declare-styleable>
    <declare-styleable name="PtrFrameLayout">
        <!-- Optional. If you put header and content in xml, you can you these to specify them. -->
        <attr format="reference" name="ptr_header"/>
        <attr format="reference" name="ptr_content"/>

        <!-- the resistance when you are moving the frame -->
        <attr format="float" name="ptr_resistance"/>

        <!-- the ration of the height of the header to trigger refresh -->
        <attr format="float" name="ptr_ratio_of_header_height_to_refresh"/>

        <attr format="integer" name="ptr_duration_to_close"/>
        <attr format="integer" name="ptr_duration_to_close_header"/>

        <!-- pull to refresh, otherwise release to refresh, default is release to refresh -->
        <attr format="boolean" name="ptr_pull_to_fresh"/>

        <!-- keep header when refreshing-->
        <attr format="boolean" name="ptr_keep_header_when_refresh"/>
    </declare-styleable>
</resources>